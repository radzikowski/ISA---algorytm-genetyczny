    <script type="text/javascript" src="/web/js/lib/backbone.js"> </script>
    <script type="text/javascript" src="/web/js/lib/jquery-1.7.min.js"> </script>
    <script type="text/javascript" src="/web/js/lib/require-jquery.js"> </script>
